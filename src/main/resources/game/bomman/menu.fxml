<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.image.Image?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="285.0" prefWidth="412.0" xmlns="http://javafx.com/javafx/18" xmlns:fx="http://javafx.com/fxml/1" fx:controller="game.bomman.MenuController">
   <children>
      <ImageView fitHeight="395.0" fitWidth="634.0" layoutX="2.0" layoutY="1.0" pickOnBounds="true" preserveRatio="true">
         <image>
            <Image url="@assets/sprites/start_panel.png" />
         </image>
      </ImageView>
      <Button layoutX="280.0" layoutY="218.0" mnemonicParsing="false" onAction="#startGameClicked" opacity="0.96" text="Start Game" textAlignment="CENTER">
         <font>
            <Font size="13.0" />
         </font>
      </Button>
      <Button layoutX="301.0" layoutY="295.0" mnemonicParsing="false" onAction="#exitClicked" text="Exit" textAlignment="CENTER">
         <font>
            <Font size="13.0" />
         </font>
      </Button>
      <Button layoutX="281.0" layoutY="257.0" mnemonicParsing="false" onAction="#instructionClicked" text="Instruction">
         <font>
            <Font size="13.0" />
         </font>
      </Button>
   </children>
</AnchorPane>
